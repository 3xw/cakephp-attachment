{
  "name": "3xw/cakephp-attachment",
  "description": "New Attachment plugin for CakePHP",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=8.1",
    "cakephp/cakephp": "^5.0",
    "friendsofcake/crud": "^7.0",
    "friendsofcake/search": "^7.0",
    "3xw/cakephp-utils":"^5.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "league/flysystem-ziparchive": "^1.0",
    "intervention/image": "^2.3",
    "firebase/php-jwt": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Trois\\Attachment\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trois\\Attachment\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"

    }
  },
  "minimum-stability": "dev"
}
